{
  "name": "react-electron-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate for creating project that based on Electron and React.js",
  "repository": "git@github.com:minhnhatspk/react-electron-boilerplate.git",
  "main": "src/init.js",
  "author": "Nhat Hoang Minh Pham <nhathoangminhpham@gmail.com>",
  "contributors": [
    "miZyind <mizyind@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently --kill-others-on-fail \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose --watchAll=false"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "electron-is-dev": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^3.0.2",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-prebuilt-compile": "^3.0.0",
    "redux-mock-store": "^1.5.3",
    "wait-on": "^3.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
